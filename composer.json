{
    "name": "phpextra/event-manager",
    "description": "Event manager based on reflection, annotations and priority queue",
    "keywords": ["event manager", "event", "listener", "observer", "psr-0"],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/phpextra/event-manager",
    "support": {
        "issues": "https://github.com/phpextra/event-manager/issues",
        "source": "https://github.com/phpextra/event-manager"
    },
    "authors": [
        {
            "name": "Jacek Kobus",
            "email": "kobus.jacek@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "PHPExtra\\EventManager": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "psr/log": "~1.0",
        "zendframework/zend-stdlib": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "archive": {
        "exclude": ["/*", "!/src", "!.gitignore", "!LICENSE.md", "!README.md"]
    }
}
